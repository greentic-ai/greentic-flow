id: adapter_ok
type: messaging

nodes:
  in:
    messaging.telegram.sendMessage:
      chat_id: "1234"
      text: "Hello from adapter_ok"
    routing:
      - to: email_notify

  email_notify:
    email.google.send:
      to: "team@greentic.dev"
      subject: "Adapter OK"
      body: "Adapters resolved successfully."
    routing:
      - out: true
